<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
        "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="Person" class="cc.rooho.more.ioc.Person">
        <property name="name">
            <value>小龙</value>
        </property>
        <property name="age">
            <value>23</value>
        </property>
        <property name="grade">
            <ref local="Grade"/>
        </property>
    </bean>
    <bean id="Grade" class="cc.rooho.more.ioc.Grade">
        <property name="math">
            <value>99</value>
        </property>
        <property name="english">
            <value>59</value>
        </property>
    </bean>
</beans>
